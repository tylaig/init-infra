direitos_setup() {
    echo -e "$amarelo===================================================================================================\e[0m"
    echo -e "$amarelo=                                                                                                 =\e[0m"
    echo -e "$amarelo=  $branco Este auto instalador foi desenvolvido para auxiliar na instala√ß√£o das principais aplica√ß√µes $amarelo  =\e[0m"
    echo -e "$amarelo=  $branco  dispon√≠veis no mercado open source. J√° deixo todos os cr√©ditos aos desenvolvedores de cada $amarelo  =\e[0m"
    echo -e "$amarelo=  $branco aplica√ß√£o dispon√≠veis aqui. Este Setup √© licenciado sob a Licen√ßa MIT (MIT). Voc√™ pode usar, $amarelo =\e[0m"
    echo -e "$amarelo=  $branco  copiar, modificar, integrar, publicar, distribuir e/ou vender c√≥pias dos produtos finais,  $amarelo  =\e[0m"
    echo -e "$amarelo=  $branco   mas deve sempre declarar que SetupMSAPP √© o autor original destes c√≥digos e atribuir um link $amarelo  =\e[0m"
    echo -e "$amarelo=  $branco           para o reposit√≥rio oficial: https://github.com/tylaig/init-infra           $amarelo  =\e[0m"
    echo -e "$amarelo=                                                                                                 =\e[0m"
    echo -e "$amarelo===================================================================================================\e[0m"
    echo ""
    echo ""
}

direitos_instalador() {
    # Pula a confirma√ß√£o e vai direto para o menu
    echo "SetupMSAPP - Instalador de Aplica√ß√µes para Infraestrutura"
    echo "Licenciado sob MIT - https://github.com/tylaig/init-infra"
    echo ""
    echo "Iniciando instalador..."
    sleep 1
    return
}

