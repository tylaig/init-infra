## Menu de ferramentas

nome_menu() {
    clear
    echo -e "$amarelo===================================================================================================\e[0m"
    echo ""
    echo -e "$branco                    โโโโ   โโโโโโโโโโโโโโโโ   โโโโโโ   โโโ    โโโโโโโ โโโโโโโโ                \e[0m"
    echo -e "$branco                    โโโโโ โโโโโโโโโโโโโโโโโโ  โโโโโโ   โโโ    โโโโโโโโโโโโโโโโ                \e[0m"
    echo -e "$branco                    โโโโโโโโโโโโโโโโโ  โโโโโโ โโโโโโ   โโโ    โโโ  โโโโโโโโโ                  \e[0m"
