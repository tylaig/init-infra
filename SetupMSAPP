## Titulo Saindo do setup

nome_saindo() {
    clear
    echo ""
    echo -e "$branco                โโโโโโโโ โโโโโโ โโโโโโโ   โโโโโโโโโโ  โโโโโโโ     โโโโโโโ  โโโโโโโ \e[0m"
    echo -e "$branco                โโโโโโโโโโโโโโโโโโโโโโโโ  โโโโโโโโโโโโโโโโโโโโ    โโโโโโโโโโโโโโโโโ\e[0m"
    echo -e "$branco                โโโโโโโโโโโโโโโโโโโโโโโโโ โโโโโโ  โโโโโโ   โโโ    โโโ  โโโโโโ   โโโ\e[0m"
    echo -e "$branco                โโโโโโโโโโโโโโโโโโโโโโโโโโโโโโโโ  โโโโโโ   โโโ    โโโ  โโโโโโ   โโโ\e[0m"
    echo -e "$branco                โโโโโโโโโโโ  โโโโโโโโโ โโโโโโโโโโโโโโโโโโโโโโโ    โโโโโโโโโโโโโโโโโ\e[0m"
