    echo -e "$amarelo=  $branco suas proximas instala√ß√µes, voc√™ precisar√° informar √†s credenciais de acesso do seu portainer $amarelo =\e[0m"
    echo -e "$amarelo=                                                                                                 =\e[0m"
    echo -e "$amarelo===================================================================================================\e[0m"
    echo ""
    echo ""
    
}

