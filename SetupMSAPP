menu_comandos(){
  ## Portainer
  echo -e "> ${verde}Gerenciamento de Serviรงos:${reset}"
  echo -e "${branco} โข ${amarelo}portainer.restart${reset} - ${branco}Reinicia o Portainer${reset}"
  echo -e "${branco} โข ${amarelo}portainer.reset${reset} - ${branco}Reseta a senha do Portainer${reset}"
  echo -e "${branco} โข ${amarelo}portainer.update${reset} - ${branco}Atualiza o Portainer${reset}"
  echo -e "${branco} โข ${amarelo}traefik.update${reset} - ${branco}Atualiza o Traefik${reset}"
  echo -e "${branco} โข ${amarelo}traefik.dash${reset} - ${branco}Ativa o Dashboard do Traefik${reset}"
  echo ""

  ## Monitoramento
  echo -e "> ${verde}Comandos de Monitoramento:${reset}"
  echo -e "${branco} โข ${amarelo}ctop${reset} - ${branco}Instala o CTOP${reset}"
  echo -e "${branco} โข ${amarelo}htop${reset} - ${branco}Instala o HTOP${reset}"
  echo ""

  ## Chatwoot
  echo -e "> ${verde}Comandos do Chatwoot:${reset}"
  echo -e "${branco} โข ${amarelo}chatwoot.mail${reset} - ${branco}Troca os Emails do Chatwoot pela versรฃo do Orion${reset}"
  echo -e "${branco} โข ${amarelo}chatwoot.n.mail${reset} - ${branco}Troca os Emails do Chatwoot Mega pela versรฃo do Orion${reset}"
  echo ""

  ## Ferramentas
  echo -e "> ${verde}Comandos de Ferramentas:${reset}"
  echo -e "${branco} โข ${amarelo}evolution.v1${reset} - ${branco}Instala a Evolution v1.8+${reset}"
  echo -e "${branco} โข ${amarelo}evolution.lite${reset} - ${branco}Instala a Evolution Lite${reset}"
  echo -e "${branco} โข ${amarelo}transcrevezap${reset} - ${branco}Instala o Transcreve Zap${reset}"
  echo -e "${branco} โข ${amarelo}minio.bucket${reset} - ${branco}Cria Buckets Publicas no MinIO${reset}"
  echo ""

  ## Quepasa
  echo -e "> ${verde}Comandos do Quepasa:${reset}"
  echo -e "${branco} โข ${amarelo}quepasa.setup.off${reset} - ${branco}Desativa o Setup do Quepasa${reset}"
  echo -e "${branco} โข ${amarelo}quepasa.setup.on${reset} - ${branco}Ativa o Setup do Quepasa${reset}"
  echo ""

  ## Manutenรงรฃo
  echo -e "> ${verde}Comandos de Manutenรงรฃo:${reset}"
  echo -e "${branco} โข ${amarelo}limpar${reset} - ${branco}Limpa Logs, volumes e imagens do Docker nรฃo usadas${reset}"

  echo -e ""
  echo -e "${branco}<-- Digite ${amarelo}P1 ${branco}para ir para pagina 1             ${amarelo}|${branco}              Digite ${amarelo}P2${branco} para ir para pagina 2 -->${reset}"
  echo -e ""
}

