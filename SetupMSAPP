    echo "SetupMSAPP - Instalador de Aplica√ß√µes para Infraestrutura"
    echo ""
    echo "Uso: $0 [OP√á√ïES] [APLICA√á√ÉO] [PAR√ÇMETROS]"
    echo ""
    echo "OP√á√ïES:"
    echo "  -h, --help              Mostra esta ajuda"
    echo "  -v, --version           Mostra a vers√£o"
