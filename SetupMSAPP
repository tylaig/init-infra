show_version() {
    echo "SetupMSAPP v2.7.1"
    echo "Instalador de Aplicaรงรตes para Infraestrutura"
    echo "Suporte para execuรงรฃo via SSH com parรขmetros CLI"
}

# Processa argumentos da linha de comando
process_args() {
    local current_param=""
    
    while [[ $# -gt 0 ]]; do
        case $1 in
            -h|--help)
                show_help
                exit 0
                ;;
            -v|--version)
                show_version
                exit 0
                ;;
            -a|--auto)
                AUTO_MODE=true
                shift
                ;;
            -s|--skip-license)
                SKIP_LICENSE=true
                shift
                ;;
            -q|--quiet)
                QUIET_MODE=true
                shift
                ;;
            -r|--return-data)
                RETURN_DATA=true
                shift
                ;;
            -c|--clean-install)
                CLEAN_INSTALL=true
                shift
                ;;
            --menu)
                CLI_OPTION="$2"
                menu_instalador="$2"
                shift 2
                ;;
            --domain|--user|--password|--server-name|--network|--email|--db-name|--db-user|--db-password|--admin-email|--admin-password|--bucket-name|--api-key|--webhook-url|--token|--secret-key|--access-key|--redis-password|--mysql-password|--postgres-password)
                current_param="$1"
                CLI_PARAMS+=("$1" "$2")
                shift 2
                ;;
            -*)
                echo "Opรงรฃo desconhecida: $1"
                echo "Use --help para ver as opรงรตes disponรญveis"
                exit 1
                ;;
            *)
                if [[ -z "$CLI_OPTION" ]]; then
                    CLI_OPTION="$1"
                else
                    echo "Erro: Mรบltiplas aplicaรงรตes especificadas"
                    exit 1
                fi
                shift
                ;;
        esac
    done
}

# Funรงรฃo para definir variรกveis baseadas nos parรขmetros CLI
set_cli_variables() {
    local i=0
    while [[ $i -lt ${#CLI_PARAMS[@]} ]]; do
        case "${CLI_PARAMS[$i]}" in
            --domain)
                url_portainer="${CLI_PARAMS[$((i+1))]}"
                url_chatwoot="${CLI_PARAMS[$((i+1))]}"
                url_evolution="${CLI_PARAMS[$((i+1))]}"
                url_minio="${CLI_PARAMS[$((i+1))]}"
                url_typebot="${CLI_PARAMS[$((i+1))]}"
                url_n8n="${CLI_PARAMS[$((i+1))]}"
                url_flowise="${CLI_PARAMS[$((i+1))]}"
                url_pgadmin="${CLI_PARAMS[$((i+1))]}"
                url_nocobase="${CLI_PARAMS[$((i+1))]}"
                url_botpress="${CLI_PARAMS[$((i+1))]}"
                url_wordpress="${CLI_PARAMS[$((i+1))]}"
                url_baserow="${CLI_PARAMS[$((i+1))]}"
                url_mongodb="${CLI_PARAMS[$((i+1))]}"
                url_rabbitmq="${CLI_PARAMS[$((i+1))]}"
                url_uptimekuma="${CLI_PARAMS[$((i+1))]}"
                url_calcom="${CLI_PARAMS[$((i+1))]}"
                url_mautic="${CLI_PARAMS[$((i+1))]}"
                url_appsmith="${CLI_PARAMS[$((i+1))]}"
                url_qdrant="${CLI_PARAMS[$((i+1))]}"
                url_woofedcrm="${CLI_PARAMS[$((i+1))]}"
                url_formbricks="${CLI_PARAMS[$((i+1))]}"
                url_nocodb="${CLI_PARAMS[$((i+1))]}"
                url_langfuse="${CLI_PARAMS[$((i+1))]}"
                url_metabase="${CLI_PARAMS[$((i+1))]}"
                url_odoo="${CLI_PARAMS[$((i+1))]}"
                url_chatwoot_nestor="${CLI_PARAMS[$((i+1))]}"
                url_unoapi="${CLI_PARAMS[$((i+1))]}"
                url_n8n_quepasa="${CLI_PARAMS[$((i+1))]}"
                url_quepasa="${CLI_PARAMS[$((i+1))]}"
                ;;
            --user)
                user_portainer="${CLI_PARAMS[$((i+1))]}"
                ;;
            --password)
                pass_portainer="${CLI_PARAMS[$((i+1))]}"
                ;;
            --server-name)
                nome_servidor="${CLI_PARAMS[$((i+1))]}"
                ;;
            --network)
                nome_rede_interna="${CLI_PARAMS[$((i+1))]}"
                ;;
            --email)
                email_admin="${CLI_PARAMS[$((i+1))]}"
                ;;
            --db-name)
                db_name="${CLI_PARAMS[$((i+1))]}"
                ;;
            --db-user)
                db_user="${CLI_PARAMS[$((i+1))]}"
                ;;
            --db-password)
                db_password="${CLI_PARAMS[$((i+1))]}"
                ;;
            --admin-email)
                admin_email="${CLI_PARAMS[$((i+1))]}"
                ;;
            --admin-password)
                admin_password="${CLI_PARAMS[$((i+1))]}"
                ;;
            --bucket-name)
                bucket_name="${CLI_PARAMS[$((i+1))]}"
                ;;
            --api-key)
                api_key="${CLI_PARAMS[$((i+1))]}"
                ;;
            --webhook-url)
                webhook_url="${CLI_PARAMS[$((i+1))]}"
                ;;
            --token)
                token="${CLI_PARAMS[$((i+1))]}"
                ;;
            --secret-key)
                secret_key="${CLI_PARAMS[$((i+1))]}"
                ;;
            --access-key)
                access_key="${CLI_PARAMS[$((i+1))]}"
                ;;
            --redis-password)
                redis_password="${CLI_PARAMS[$((i+1))]}"
                ;;
            --mysql-password)
                mysql_password="${CLI_PARAMS[$((i+1))]}"
                ;;
            --postgres-password)
                postgres_password="${CLI_PARAMS[$((i+1))]}"
                ;;
        esac
        i=$((i+2))
    done
}

# Funรงรฃo para retornar dados apรณs instalaรงรฃo
return_installation_data() {
    local app="$1"
    local timestamp=$(date '+%Y-%m-%d %H:%M:%S')
    
    echo ""
    echo "=== DADOS DA INSTALAรรO ==="
    echo "Timestamp: $timestamp"
    echo "Aplicaรงรฃo: $app"
    echo "Status: Instalaรงรฃo concluรญda com sucesso"
    echo ""
    
    case "$app" in
        "01"|"traefik-portainer")
            echo "URL do Portainer: https://$url_portainer"
            echo "Usuรกrio: $user_portainer"
            echo "Senha: [CONFIGURADA]"
            echo "Nome do Servidor: $nome_servidor"
            echo "Rede Interna: $nome_rede_interna"
            echo "Email Admin: $email_admin"
            ;;
        "02"|"chatwoot")
            echo "URL do Chatwoot: https://$url_chatwoot"
            echo "Banco de Dados: $db_name"
            echo "Usuรกrio DB: $db_user"
            echo "Email Admin: $admin_email"
            echo "Senha Admin: [CONFIGURADA]"
            ;;
        "08"|"n8n")
            echo "URL do N8N: https://$url_n8n"
            echo "Banco de Dados: $db_name"
            echo "Usuรกrio DB: $db_user"
            ;;
        "06"|"minio")
            echo "URL do MinIO: https://$url_minio"
            echo "Bucket: $bucket_name"
            echo "Access Key: $access_key"
            echo "Secret Key: [CONFIGURADA]"
            ;;
        *)
            echo "URL: https://$url_portainer"
            echo "Configuraรงรฃo personalizada aplicada"
            ;;
    esac
    
    echo ""
    echo "=== FIM DOS DADOS ==="
}

